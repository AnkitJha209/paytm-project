generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int       @default(autoincrement()) @id
  username  String    @unique   @db.VarChar(30)
  email     String
  firstName String    
  lastName  String?
  phoneNo   Int?
  password  String    @db.VarChar(20)    
}
